<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane minHeight="300.0" minWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.MainController">
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="tfResult" prefHeight="52.0" prefWidth="300.0" />
            <HBox fx:id="hbox1" prefHeight="48.0" prefWidth="800.0">
               <children>
                  <Button fx:id="btnPlus" mnemonicParsing="false" onMouseClicked="#setOp" prefHeight="48.0" prefWidth="45.0" text="+" />
               </children>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </HBox>
            <HBox fx:id="hbox2" prefHeight="48.0" prefWidth="200.0">
               <children>
                  <Button fx:id="btnMinus" mnemonicParsing="false" onMouseClicked="#setOp" prefHeight="48.0" prefWidth="45.0" text="-" />
               </children>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </HBox>
            <HBox fx:id="hbox3" prefHeight="48.0" prefWidth="200.0">
               <padding>
                  <Insets left="50.0" />
               </padding>
               <children>
                  <Button fx:id="btnMult" mnemonicParsing="false" onMouseClicked="#setOp" prefHeight="48.0" prefWidth="45.0" text="x" />
               </children>
            </HBox>
            <HBox fx:id="hbox4" prefHeight="48.0" prefWidth="200.0">
               <children>
                  <Button fx:id="btnDiv" mnemonicParsing="false" onMouseClicked="#setOp" prefHeight="48.0" prefWidth="45.0" text="/" />
                  <Button fx:id="btnDel" mnemonicParsing="false" onMouseClicked="#delNum" prefHeight="48.0" prefWidth="45.0" text="DEL" />
                  <Button fx:id="btnResult" mnemonicParsing="false" onMouseClicked="#resultFromOp" prefHeight="48.0" prefWidth="45.0" text="=" />
               </children>
               <padding>
                  <Insets left="50.0" />
               </padding>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
